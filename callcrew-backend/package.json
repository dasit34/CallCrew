{
  "name": "callcrew-backend",
  "version": "1.0.0",
  "description": "AI Phone Receptionist System using Twilio, OpenAI, and MongoDB",
  "main": "server.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
  "start": "node server.js",
  "dev": "nodemon server.js",
  "seed": "node scripts/seedIndustries.js",
  "build:frontend": "cd ../callcrew-dashboard && npm ci && npm run build",
  "sim:batch": "TS_NODE_TRANSPILE_ONLY=1 ts-node internal/simulator/runBatchSimulation.ts"
  },
  "keywords": [
    "twilio",
    "openai",
    "ai-receptionist",
    "phone-system"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "nodemailer": "^6.9.7",
    "openai": "^4.24.1",
    "twilio": "^4.19.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.0",
    "@types/node": "^20.10.0"
  }
}
